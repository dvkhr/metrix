<!doctype html>
<html lang='en'>
<head>
    <meta charset='utf-8'>
    <title>Metrics list</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>Value</th>
        </tr>
        {{ range $mName, $mVal := . }}
            <tr>
                <td>{{ $mName }}</td>
                <td>{{ $mVal }}</td>
            </tr>
        {{ end }}
    </table>
</body>
</html>